{
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "yaml",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.eslint.fixAll": true
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "echarts",
    "htmls",
    "jsesc",
    "layui",
    "swiper"
  ]
}
